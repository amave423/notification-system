{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "MassTransit": "Information"
    }
  },
  "RabbitMQ": {
    "Host": "rabbitmq",
    "VirtualHost": "/",
    "Username": "guest",
    "Password": "guest",
    "SmsQueue": "sms.notifications",
    "RetryQueue": "sms.notifications.retry",
    "MaxRetries": 3
  },
  "SmsSettings": {
    "SimulateDelay": true,
    "DelayMilliseconds": 1000,
    "SuccessRate": 0.95,
    "MaxMessageLength": 1600,
    "DefaultSender": "SERVICE"
  },
  "AllowedHosts": "*"
}